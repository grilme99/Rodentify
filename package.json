{
	"name": "rodentify",
	"version": "1.0.0",
	"description": "A package for authenticating Roblox game servers.",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"prepare": "npm run build",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/grilme99/Rodentify.git"
	},
	"keywords": [
		"roblox",
		"server",
		"authentication",
		"typescript",
		"rocheck",
		"validation",
		"api"
	],
	"author": "Brooke Rhodes",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/grilme99/Rodentify/issues"
	},
	"homepage": "https://github.com/grilme99/Rodentify#readme",
	"devDependencies": {
		"@types/node": "^14.11.5",
		"@types/phin": "^3.4.0",
		"prettier": "^1.13.2",
		"typescript": "^4.0.0"
	},
	"files": [
		"lib/**/*"
	],
	"dependencies": {
		"phin": "^3.5.0"
	}
}
