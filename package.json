{
  "name": "rodentify",
  "version": "1.0.0",
  "description": "A package for authenticating Roblox game servers.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grilme99/Rodentify.git"
  },
  "keywords": [
    "roblox",
    "server",
    "authentication",
    "typescript",
    "rocheck",
    "validation",
    "api"
  ],
  "author": "Brooke Rhodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grilme99/Rodentify/issues"
  },
  "homepage": "https://github.com/grilme99/Rodentify#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "codecov": "^3.8.0",
    "jest": "^26.5.0",
    "prettier": "^1.13.2",
    "ts-jest": "^26.4.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
